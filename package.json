{
  "name": "clo-ui",
  "version": "0.5.5",
  "license": "Apache-2.0",
  "description": "CLO common library: components, utils and hooks",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": [
    "dist/styles/**/*.css"
  ],
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup -c --bundleConfigAsCjs && node scripts/generate-legacy-entries.cjs",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "yarn build",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "test": "jest"
  },
  "files": [
    "dist",
    "components",
    "hooks",
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./components/*": {
      "types": "./dist/types/components/*/index.d.ts",
      "import": "./dist/esm/components/*.js",
      "require": "./dist/cjs/components/*.js"
    },
    "./hooks/*": {
      "types": "./dist/types/hooks/*.d.ts",
      "import": "./dist/esm/hooks/*.js",
      "require": "./dist/cjs/hooks/*.js"
    },
    "./utils/*": {
      "types": "./dist/types/utils/*.d.ts",
      "import": "./dist/esm/utils/*.js",
      "require": "./dist/cjs/utils/*.js"
    },
    "./styles/*": {
      "default": "./dist/styles/*"
    }
  },
  "typesVersions": {
    "*": {
      "components/*": [
        "dist/types/components/*/index.d.ts"
      ],
      "hooks/*": [
        "dist/types/hooks/*.d.ts"
      ],
      "utils/*": [
        "dist/types/utils/*.d.ts"
      ],
      "*": [
        "dist/types/*.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "react-date-range": "1.4.0",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-date-range": "^1.4.9",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^10.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "6.1.0",
    "rollup": "^4.52.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.93.2",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "resolutions": {
    "@types/minimatch": "5.1.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
